<script>
        <%if(flash.success && flash.success.length>0){%>

            new Noty({

                  text:"<%=flash.success%>",
                  theme:'relax',
                  layout:'topRight',
                  type:'success',
                  timeout:1500
                
            }).show();


        <%}%>   
        
        <%if(flash.error && flash.error.length>0){%>
 
new Noty({

      text:"<%=flash.error%>",
      theme:'relax',
      layout:'topRight',
      type:'error',
      timeout:1500
    
}).show();


<%}%>    

<%if(flash.info && flash.info.length>0){%>

new Noty({

      text:"<%=flash.info%>",
      theme:'relax',
      type:'info',
      layout:'topRight',
      timeout:1500
    
}).show();


<%}%>    

</script>

